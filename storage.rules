rules_version = '2';
service firebase.storage {
  match /b/{bucket}/o {

    match /videos/{videoId}/{fileName} {
      allow read: if true;
      allow write: if request.auth != null;
    }

    match /thumbnails/{videoId}/{fileName} {
      allow read: if true;
      allow write: if request.auth != null;
    }
  }
}
